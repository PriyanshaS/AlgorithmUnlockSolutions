#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
        int t;
        string x1,y1,x2,y2;
        cin>>t;
        while(t--){
                cin>>x1>>x2>>y1>>y2;
                if(x1.length()>x2.length())
                cout<<"Soumy-Sigma\n";
                else if(x1.length()<x2.length())
                cout<<"Soumy-Beta\n";
                else if(x1>x2)
                cout<<"Soumy-Sigma\n";
                else
                cout<<"Soumy-Beta\n";
        }
	return 0;
}
